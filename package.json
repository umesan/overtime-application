{
  "name": "OVERTIME_APPLICATION",
  "description": "残業さん:万屋一家シリーズの勤怠管理システム 勤之助ver.2 の残業申請入力を支援する非公式ツール",
  "version": "3.0.0",
  "author": "@umesan",
  "main": "",
  "keywords": [
    "残業怖い",
    "残業つらい",
    "残業やばい",
    "残業苦しい",
    "残業助けて",
    "残業もういや"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/umesan/overtime-application.git"
  },
  "scripts": {
    "start": "better-npm-run start",
    "start:bookmark": "better-npm-run start:bookmark",
    "deploy": "better-npm-run deploy",
    "deploy:bookmark": "better-npm-run deploy:bookmark"
  },
  "betterScripts": {
    "start": {
      "command": "rimraf dist && node utils/webserver.js",
      "env": {}
    },
    "start:bookmark": {
      "command": "yarn start",
      "env": {
        "TARGET": "bookmarklet"
      }
    },
    "deploy": {
      "command": "rimraf dist chrome_extension && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:bookmark": {
      "command": "rimraf dist docs && yarn deploy",
      "env": {
        "TARGET": "bookmarklet"
      }
    }
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.14",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "jquery": "^3.1.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^4.0.0",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "scriptjs": "^2.5.8",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "write-file-webpack-plugin": "^3.4.2"
  }
}
